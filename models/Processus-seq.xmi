<?xml version="1.0" encoding="UTF-8"?>
<LDP:Processus
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:LDP="http://www.univ-pau.fr/m2ti/2020/LDP"
    xsi:schemaLocation="http://www.univ-pau.fr/m2ti/2020/LDP LDP.ecore">
  <activites
      description="Addition">
    <action
        methodName="addition"
        returnTag="res1">
      <paramsTag>x1</paramsTag>
      <paramsTag>y1</paramsTag>
    </action>
  </activites>
  <activites
      description="Soustraction">
    <action
        methodName="substraction"
        returnTag="res2">
      <paramsTag>x2</paramsTag>
      <paramsTag>y2</paramsTag>
    </action>
  </activites>
  <activites
      description="Multiplication">
    <action
        methodName="multiplication"
        returnTag="res">
      <paramsTag>res1</paramsTag>
      <paramsTag>res2</paramsTag>
    </action>
  </activites>
  <debut/>
  <fin/>
</LDP:Processus>
